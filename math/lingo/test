model:
	min=x12 * 2 + x13 * 3 + x14 * 4
		+ x21 * 2 + x23 * 5 + x24 * 6
		+ x31 * 3 + x32 * 5 + x34 * 7
		+ x41 * 4 + x42 * 6 + x43 * 7;
	@bin(x12);@bin(x13);@bin(x14);
	@bin(x21);@bin(x23);@bin(x24);
	@bin(x31);@bin(x32);@bin(x34);
	@bin(x41);@bin(x42);@bin(x43);
	x12 + x21 <= 1;
	x13 + x31 <= 1;
	x14 + x41 <= 1;
	x23 + x32 <= 1;
	x24 + x42 <= 1;
	x34 + x43 <= 1;
	x12 + x13 + x14 = 1;
	x21 + x23 + x24 = 1;
	x31 + x32 + x34 = 1;
	x41 + x42 + x43 = 1;
	x21 + x31 + x41 = 1;
	x12 + x32 + x42 = 1;
	x13 + x23 + x43 = 1;
	x14 + x24 + x34 = 1;
end 
